{
		Name:         {{if .Alias}}{{.Alias | printf "%q"}}{{else}}{{print "yandex_" (.Service | snake) "_" (.Resource | snake) "_" (.AbsolutePath | asFullFieldName | join "" | snake) | printf "%q"}}{{end}},
		Resolver:     fetch{{.Service}}{{.Resource}}{{.AbsolutePath | asFullFieldName | join ""}},
		Multiplex:    {{.Multiplex}},
		IgnoreError:  client.IgnoreErrorHandler,
		DeleteFilter: client.DeleteFolderFilter,
		Columns: []schema.Column{
{{range .Columns}}{{template "column.go.tmpl" .}}{{end}}
		},
{{with .Relations}}
		Relations: []*schema.Table{
{{range .}}{{template "relation.go.tmpl" .}}{{end}}
		},
{{end}}
},
